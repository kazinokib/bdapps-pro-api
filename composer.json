{
    "name": "kazinokib/bdappsapi",
    "description": "A Laravel package for BDApps API integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kazi Mohammad Nokib",
            "email": "nokibevon7@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kazinokib\\BdappsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kazinokib\\BdappsApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kazinokib\\BdappsApi\\BdappsApiServiceProvider"
            ],
            "aliases": {
                "BdappsApi": "Kazinokib\\BdappsApi\\Facades\\BdappsApi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
